{
  "name": "vue-roughviz",
  "version": "0.1.0",
  "description": "Use the fun and informal charting capabilities of Rough-Viz charting in Vue.js",
  "main": "dist/vue-roughviz.ssr.js",
  "browser": "dist/vue-roughviz.esm.js",
  "module": "dist/vue-roughviz.esm.js",
  "unpkg": "dist/vue-roughviz.min.js",
  "types": "vue-roughviz.d.ts",
  "files": [
    "dist/*",
    "vue-roughviz.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "storybook:debug": "run-p vue-devtools storybook:serve"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@storybook/addon-actions": ">=5.3.0",
    "@storybook/addon-knobs": ">=5.3.0",
    "@storybook/addon-links": ">=5.3.0",
    "@storybook/addon-notes": ">=5.3.0",
    "@storybook/vue": ">=5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/composition-api": "1.0.0-beta.14",
    "@vue/devtools": "^5.3.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "rough-viz": "^1.0.5",
    "typescript": "~4.0.3",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "~1.2.1",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "1.0.0-beta.14",
    "rough-viz": "^1.0.5",
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
